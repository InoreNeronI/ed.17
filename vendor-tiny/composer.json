{
  "name": "isei-ivei/probak-pruebas",
  "author": "ISEI - IVEI | Irakas Sistema Ebaluatu eta Ikertzeko Erakundea - Instituto Vasco de Evaluación e Investigación Educativa",
  "homepage": "http://ediagnostikoak.net/",
  "require": {
    "php": "^5.4.0|^7.0",
    "ext-pdo_mysql": "*",
    "ext-pdo_sqlite": "*",
    "doctrine/dbal": "@stable",
    "symfony/event-dispatcher": "@stable",
    "symfony/http-kernel": "@stable",
    "symfony/http-foundation": "@stable",
    "symfony/routing": "@stable",
    "symfony/yaml": "@stable",
    "twig/twig": "@stable"
  },
  "require-dev": {
    "andig/dbcopy": "@dev",
    "symfony/var-dumper": "@stable"
  },
  "scripts": {
    "post-install-cmd": ["@composer dump-autoload --optimize"],
    "post-update-cmd": ["@composer dump-autoload --optimize"]
  },
  "autoload": {
    "psr-4": {"": "../src/"}
  },
  "minimum-stability": "stable",
  "extra": {
    "incenteev-parameters": [{
      "file": "app/config/connection.yml",
      "dist-file": "app/config/connection.dist.yml"}]
  },
  "config": {
    "bin-dir": "../bin",
    "platform": {"php": "5.4.0"},
    "preferred-install": "dist",
    "vendor-dir": "./"
  }
}