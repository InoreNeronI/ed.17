{
  "name": "isei-ivei/ed17probak",
  "type": "project",
  "description": "Irakas Sistema Ebaluatu eta Ikertzeko Erakundea | Instituto Vasco de Evaluación e Investigación Educativa",
  "autoload": {
    "classmap": [ "lib/" ]
  },
  "require": {
    "php": "^5.4.0|^7.0",
    "catfan/medoo": "@stable",
    "incenteev/composer-parameter-handler": "@stable",
    "twig/twig": "@stable",
    "symfony/http-kernel": "~2.3|~3.0",
    "symfony/routing": "~2.3|~3.0"
  },
  "authors": [{
    "name": "Martin Mozos",
    "email": "mmozos@isei-ivei.net"
  }],
  "minimum-stability": "stable",
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    ],
    "post-install-cmd": [
      "@symfony-scripts",
      "npm install bower",
      "node node_modules/bower/bin/bower install"
    ],
    "post-update-cmd": [
      "@symfony-scripts",
      "npm update bower",
      "node node_modules/bower/bin/bower update"
    ]
  },
  "extra": {
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  },
  "config": {
    "platform": {
      "php": "5.4.0"
    }
  }
}
