{
  "name": "isei-ivei/probak-pruebas",
  "author": "ISEI - IVEI | Irakas Sistema Ebaluatu eta Ikertzeko Erakundea - Instituto Vasco de Evaluación e Investigación Educativa",
  "homepage": "http://ediagnostikoak.eus",
  "require": {
    "php": ">=5.5.9 <7.2.0",
    "ext-pdo_mysql": "*",
    "ext-pdo_sqlite": "*",
    "doctrine/dbal": "@stable",
    "kriswallsmith/assetic": "@stable",
    "symfony/dotenv": "^3.3",
    "symfony/event-dispatcher": "@stable",
    "symfony/http-kernel": "@stable",
    "symfony/http-foundation": "@stable",
    "symfony/routing": "@stable",
    "symfony/yaml": "@stable",
    "twig/twig": "@stable"
  },
  "require-dev": {
    "andig/dbcopy": "@dev",
    "incenteev/composer-parameter-handler": "@stable",
    "neronmoon/scriptsdev": "@stable",
    "phpstan/phpstan": "@stable",
    "symfony/var-dumper": "@stable",
    "tholu/php-packer": "@stable"
  },
  "scripts": {
    "post-install-cmd": ["printf \"\\n\\tVendors has been installed succesfully\\n\""],
    "post-update-cmd": ["printf \"\\n\\tVendors has been updated succesfully\\n\""],
    "pre-install-cmd": "printf \"\\n\\tInstalling vendors\\n\\n\"",
    "pre-update-cmd": "printf \"\\n\\tUpdating vendors\\n\\n\"",
    "production-install": "@install --no-ansi --no-dev --no-interaction --no-progress --no-scripts",
    "production-update": "@update --no-ansi --no-dev --no-interaction --no-progress --no-scripts"
  },
  "autoload": {
    "psr-4": {"App\\": "../src"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "incenteev-parameters": [{
      "file": "../app/connection.yml",
      "dist-file": "../app/connection.dist.yml"}
    ],
    "scripts-dev": {
      "build-parameters": ["printf \"\\n\\tBuilding parameters\\n\\n\"", "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"],
      "post-install-cmd": ["@build-parameters"],
      "post-update-cmd": ["@build-parameters"]
    }
  },
  "config": {
    "bin-dir": "../bin",
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "vendor-dir": "."
  }
}